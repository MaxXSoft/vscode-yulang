{
  "name": "yulang",
  "displayName": "YuLang",
  "description": "Syntax highlighting for YuLang.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.43.0"
  },
  "categories": [
    "Programming Languages", "Snippets"
  ],
  "publisher": "MaxXing",
  "author": {
    "name": "MaxXing"
  },
  "keywords": ["yulang", "yu", "syntax"],
  "repository": {
    "type": "git",
    "url": "https://github.com/MaxXSoft/vscode-yulang"
  },
  "bugs": {
    "url": "https://github.com/MaxXSoft/vscode-yulang/issues"
  },
  "icon": "./images/icon.png",
  "license": "MIT",
  "contributes": {
    "languages": [{
      "id": "yulang",
      "aliases": ["YuLang", "yulang"],
      "extensions": [".yu"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "yulang",
      "scopeName": "source.yu",
      "path": "./syntaxes/yulang.tmLanguage.json"
    }],
    "snippets": [{
      "language": "yulang",
      "path": "./snippets/snippets.json"
    }]
  }
}
